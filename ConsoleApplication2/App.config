<?xml version="1.0" encoding="utf-8" ?>
<configuration>
  <appSettings>
    <!--mongodb的连接参数-->
    <add key="ConnectionString" value="mongodb://localhost:27017"/>
    <add key="Database" value="MongoLog"/>
    <!--是否使用 当前调用的函数名称-->
    <add key="Function" value="1"/>
    <!--是否使用 当前线程-->
    <add key="Thread" value="1"/>
    <!--是否使用 当前所在文件-->
    <add key="File" value="1"/>
    <!--是否使用 当前所在行-->
    <add key="Line" value="1"/>
  </appSettings>
  <startup>
    <supportedRuntime version="v4.0" sku=".NETFramework,Version=v4.5" />
  </startup>
</configuration>